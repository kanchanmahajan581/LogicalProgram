package com.string;

public class Permutation {
	
	public static void printArray(int a[])
	{
		for(int i=0; i<a.length; i++)
		{
			System.out.println(a[i]+" ");
		}
		
	}
	public static void swap(int a[], int i , int j)
	{
		int temp=a[i];
		a[i]=a[j];
		a[j]=temp;		
	}
	
	
	
	
	
	
	public static void per(int a[] , int cid)
