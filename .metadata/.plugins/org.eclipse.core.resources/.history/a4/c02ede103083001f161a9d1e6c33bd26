package com.code;

public class RemoveDuplicate {
	
	public static void main(String[] args) {
		int arr[]= {12,52,63,12,45,11,63};
		int temp[]=new int[arr.length];
		
		int j=0;
		
		for(int i=0; i<arr.length-1; i++)
		{
			if(arr[i]!=arr[i+1])
			{
				temp[j]=arr[i];
				j++;
			}
		}
		for(int i=0; i<arr.length-1; i++)
		{
			System.out.println(temp[i]);
		}
	}

}
