package com.array;

import java.util.Scanner;

public class addElement {
	
	public static void main(String[] args) {
		
		int[] arr=new int [5];
		
		System.out.println("Enter The Element in array");
		Scanner sc=new Scanner(System.in);
		
		
		
		for(int i=0; i<arr.length; i++)
		{
			arr[i]=sc.nextInt();
			
		}
		 
	}

}
